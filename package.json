{
  "name": "medidiario-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts,.tsx --rule \"boundaries/entry-points:off\" --rule \"boundaries/element-types:warn\" --rule \"react-hooks/set-state-in-effect:warn\" --rule \"react-hooks/preserve-manual-memoization:warn\" --rule \"no-useless-escape:warn\"",
    "test": "vitest run",
    "test:node": "node --test tests/*.test.js tests/integration/*.test.js tests/e2e/*.test.js",
    "test:all": "npm run test && npm run test:node",
    "test:coverage": "vitest run --coverage",
    "test:coverage:gate": "npm run test:coverage",
    "test:critical": "npm run test -- tests/unit/buildPayload.test.ts tests/unit/patientDomain.test.ts tests/unit/patientUseCases.test.ts tests/unit/patientSyncMerge.test.ts tests/unit/patientSyncUseCase.test.ts tests/unit/persistenceInit.test.ts tests/unit/storageService.test.ts tests/unit/logger.test.ts",
    "test:watch": "vitest",
    "boundary:check": "node scripts/check-boundaries.mjs",
    "sync:check": "node scripts/sync-health-check.mjs",
    "quality:gate": "npm run boundary:check && npm run lint && npm run test:critical && npm run test:coverage:gate && npm run test:node && npm run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@sparticuz/chromium": "^143.0.4",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-virtual": "^3.13.18",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "firebase": "^12.8.0",
    "googleapis": "^144.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.344.0",
    "pdfjs-dist": "^4.10.38",
    "puppeteer-core": "^24.37.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "file:vendor/react-router-dom",
    "recharts": "^2.12.2",
    "tailwind-merge": "^2.2.1",
    "telegraf": "^4.16.3",
    "zod": "^4.3.5",
    "zustand": "^5.0.8"
  },
  "overrides": {
    "jws": "4.0.1",
    "qs": "6.14.1",
    "lodash": "4.17.23"
  },
  "devDependencies": {
    "@netlify/functions": "^2.8.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.10.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-pwa": "^0.19.2",
    "vitest": "^4.0.17"
  }
}
